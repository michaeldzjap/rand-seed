{
  "name": "rand-seed",
  "version": "0.1.1",
  "description": "A small library for generating random numbers",
  "main": "dist/rand-seed.js",
  "module": "dist/rand-seed.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./",
    "test": "NODE_ENV=test ./node_modules/.bin/jest",
    "build": "./node_modules/.bin/rollup --config",
    "watch": "./node_modules/.bin/rollup --config --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaeldzjap/rand-seed.git"
  },
  "keywords": [
    "random",
    "seed",
    "prng"
  ],
  "author": "Michael Dzjaparidze",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaeldzjap/rand-seed/issues"
  },
  "homepage": "https://github.com/michaeldzjap/rand-seed#readme",
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "eslint": "^5.13.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.0.0",
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^4.0.3",
    "rollup-plugin-typescript2": "^0.19.2",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "^23.10.5",
    "typescript": "^3.3.1",
    "typescript-eslint-parser": "^22.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    },
    "collectCoverage": true
  }
}
